@{
    ViewBag.Title = "About";
}
<h2>@ViewBag.Title.</h2>
<h3>@ViewBag.Message</h3>

<p>Use this area to provide additional information.</p>


<div id="map"></div>


<script type="text/javascript">
    function initMap() {
        var uluru = { lat: -25.363, lng: 131.044 };
        var map = new google.maps.Map(document.getElementById('map'), {
            zoom: 4,
            center: uluru
        });
        var marker = new google.maps.Marker({
            position: uluru,
            map: map
        });
    }
</script>
<script async defer src="http://maps.google.com/maps/api/js?key=AIzaSyB7WkyD-PKxmaWTiQffxqvfe07d8N1kdMY&callback=initMap" type="text/javascript"></script>